    "glob": "^8.0.3",
    "sass": "^1.55.0",
    "source-map-support": "^0.5.21",
    "svelte": "^3.51.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "@syncedstore/core": "^0.4.1",
    "@syncedstore/svelte": "^0.4.1",
    "@types/cron": "^2.0.0",
